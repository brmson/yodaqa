<?xml version="1.0" encoding="UTF-8" ?>
  <typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>FinalAnswerTypes</name>
    <description>FinalAnswerCAS type system</description>
    <vendor>yodaqa</vendor>
    <version>1.0</version>
    <types>
      <typeDescription>
        <name>cz.brmlab.yodaqa.model.FinalAnswer.Answer</name>
        <description>Generated candidate answer</description>
        <supertypeName>uima.cas.TOP</supertypeName>
        <features>
          <featureDescription>
            <name>text</name>
            <description>Text of the answer</description>
            <rangeTypeName>uima.cas.String</rangeTypeName>
          </featureDescription>
          <featureDescription>
            <name>confidence</name>
            <description>Confidence of the answer; this is an estimate of the probability that the answer is right</description>
            <rangeTypeName>uima.cas.Double</rangeTypeName>
          </featureDescription>
        </features>
      </typeDescription>
    </types>
  </typeSystemDescription>
